
<ul>
  {{#each sandys}}
  <li>
    <p>
      {{this.id}}. {{this.sandy}}

      <button data-planid="{{this.id}}" class="delplan">ConSUME!</button>
    </p>
  </li>
  {{/each}}
</ul>


<div class="text-center">
    <img src="https://gallery.yopriceville.com/var/resizes/Free-Clipart-Pictures/Fast-Food-PNG-Clipart/Sandwich_PNG_Clipart_Vector_Picture.png?m=1507172108"
        alt="Sandwich Picture">

    <h2>Eat-Da-Sandy</h2>


    <div class="form-group">
        <label for="userSandyName"></label>
        <textarea class="form-control" id="userSandyTextArea" rows="3"></textarea>
    </div>

    <button type="button" class="btn btn-primary">Submit</button>
    <p>Enter the full name of a type of Sandwich you want to eat, then hit enter!</p>

</div>